{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "app": {
    "title": "Robonom Blog",
    "description": "Simple blog framework",
    "author": "robonom",
    "environment": "development",
    "permalink": "pretty",
    "baseurl": "/",
    "domain_name": "",
    "name": "Robonom",
    "markdown": "kramdown",
    "highlighter": "rouge",
    "DefaultAPIAddress": "http://localhost:8090",
    "DefaultLanguage": "tr-TR",
    "ErrorPage": "~/apps/common/error.cshtml",
    "url_method_key": "get-",
    "apps_path": "/Apps",
    "asset_path": "/wwwroot/assets",
    "data_path": "/wwwroot/App_Data",
    "layouts_path": "/wwwroot/App_Data/_layouts",
    "pages_path": "/wwwroot/App_Data/pages" 
  }
}
